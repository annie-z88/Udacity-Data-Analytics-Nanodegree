   "source": [
    "tweet_data_clean = tweet_data.copy()\n",
    "twitter_archive_clean = twitter_archive.copy()\n",
    "image_predictions_clean= image_predictions.copy()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 148,
   "metadata": {},
   "outputs": [],
   "source": [
    "twitter_archive_clean = pd.merge(left=twitter_archive_clean, right=tweet_data_clean, left_on='tweet_id', right_on='tweet_id', how='right')\n",
    "\n"
   ]
  },
